<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chen.book.mapper.AreaMapper">
    <select id="queryArea" resultType="com.chen.book.entity.Area">
        SELECT
		area_id,
		area_name,
		area_desc
		FROM
		tb_area
    </select>

</mapper>